{
    "env": {
      "browser": true,
      "jest": true
    },
    "plugins": [
        "react",
        "class-property"
    ],
    "extends": ["airbnb", "plugin:prettier/recommended"],
    "parser": "babel-eslint",
    "rules": {
      "class-methods-use-this": 0,
      "import/no-named-as-default": 0,
      "no-use-before-define": 0,
      "no-param-reassign": 0,
      "react/jsx-filename-extension": [
        "error",
        {
          "extensions": [".js", ".jsx"]
        }
      ],
      "prettier/prettier": ["error", { "singleQuote": true }],
      "quotes": ["error", "single"],
      "no-console": 0
    }
  }